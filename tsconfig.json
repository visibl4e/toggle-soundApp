{
  "compilerOptions": {
    "target": "ES6",
    "useDefineForClassFields": true,
    // "module": "ESNext",
    "lib": ["DOM", "ES6", "DOM.Iterable"],
    "skipLibCheck": true,

    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true
  }
}
